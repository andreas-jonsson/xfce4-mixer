#: ../panel-plugin/mixer.desktop.in.h:2
#: ../xfce4-mixer/xfce4-mixer.desktop.in.h:2
msgid "Adjust volume levels"
msgstr "Дыбыс деңгейлерін орнату"

#: ../xfce4-mixer/xfce-mixer-container.c:219
msgid "_Playback"
msgstr "О_йнату"

#: ../xfce4-mixer/xfce-mixer-container.c:219
